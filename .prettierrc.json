{
    "plugins": ["prettier-plugin-astro", "prettier-plugin-sh", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "singleAttributePerLine": true
}
