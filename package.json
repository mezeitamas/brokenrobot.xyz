{
    "name": "brokenrobot.xyz",
    "description": "Tamas Mezei's personal website.",
    "version": "1.0.0",
    "private": true,
    "author": "Tamas Mezei",
    "repository": {
        "type": "git",
        "url": "git@github.com:mezeitamas/brokenrobot.xyz.git"
    },
    "scripts": {
        "clean": "rimraf public/ reports/ .cache/ test-results/ playwright-report/",
        "format:check": "prettier '**/*.{js,ts,tsx,json,md,yml,sh}' --check",
        "format:fix": "prettier '**/*.{js,ts,tsx,json,md,yml,sh}' --write",
        "lint:check": "eslint 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}' 'gatsby-config.ts' 'playwright.config.ts'",
        "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' 'tests/**/*.{ts,tsx}' 'gatsby-config.ts' 'playwright.config.ts'",
        "type:check": "tsc --noEmit",
        "test:unit:check": "jest",
        "test:unit:update": "jest --updateSnapshot",
        "test:e2e:check": "playwright test",
        "audit:check": "npm audit --package-lock-only --omit=dev",
        "start": "gatsby develop --port 8080",
        "build": "gatsby build",
        "serve": "gatsby serve --port 8080"
    },
    "dependencies": {
        "gatsby": "5.11.0",
        "gatsby-plugin-canonical-urls": "5.11.0",
        "gatsby-plugin-catch-links": "5.11.0",
        "gatsby-plugin-feed": "5.11.0",
        "gatsby-plugin-image": "3.11.0",
        "gatsby-plugin-manifest": "5.11.0",
        "gatsby-plugin-postcss": "6.11.0",
        "gatsby-plugin-sharp": "5.11.0",
        "gatsby-plugin-sitemap": "6.11.0",
        "gatsby-remark-autolink-headers": "6.11.0",
        "gatsby-remark-copy-linked-files": "6.11.0",
        "gatsby-remark-images": "7.11.0",
        "gatsby-remark-prismjs": "7.11.0",
        "gatsby-remark-smartypants": "6.11.0",
        "gatsby-source-filesystem": "5.11.0",
        "gatsby-transformer-remark": "6.11.0",
        "gatsby-transformer-sharp": "5.11.0",
        "prismjs": "1.29.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "4.10.1",
        "schema-dts": "1.1.2"
    },
    "devDependencies": {
        "@axe-core/playwright": "4.7.3",
        "@babel/preset-typescript": "7.22.5",
        "@playwright/test": "1.36.1",
        "@tailwindcss/typography": "0.5.9",
        "@testing-library/jest-dom": "5.17.0",
        "@testing-library/react": "14.0.0",
        "@types/jest": "29.5.3",
        "@types/node": "20.4.2",
        "@types/react": "18.2.15",
        "@types/react-dom": "18.2.7",
        "@types/react-test-renderer": "18.0.0",
        "@typescript-eslint/eslint-plugin": "6.1.0",
        "@typescript-eslint/parser": "6.1.0",
        "autoprefixer": "10.4.14",
        "babel-jest": "29.6.1",
        "babel-preset-gatsby": "3.11.0",
        "dotenv": "16.3.1",
        "eslint": "8.45.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jest": "27.2.3",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-tailwindcss": "3.13.0",
        "identity-obj-proxy": "3.0.0",
        "jest": "29.6.1",
        "jest-environment-jsdom": "29.6.1",
        "postcss": "8.4.26",
        "prettier": "3.0.0",
        "prettier-plugin-sh": "0.13.1",
        "prettier-plugin-tailwindcss": "0.4.1",
        "react-test-renderer": "18.2.0",
        "rimraf": "5.0.1",
        "tailwindcss": "3.3.3",
        "typescript": "5.1.6"
    }
}
